<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="fr.lb.l5r.gui.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="fr.lb.l5r.gui.Perspective"
            id="L5RGui.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="fr.lb.l5r.gui.editor.PlayerEditor"
            default="false"
            icon="icons/sample.gif"
            id="l5rgui.editor.PlayerEditor"
            name="Player"/>
   </extension>

</plugin>
